.page-header
  %h1 Welcome
.container
  .row
    .col-sm-12.col-md-6
      .h3 Your collection
      %ul
        - current_user.collections.each do |c|
          %li= c.board_game.name
    .col-sm-12.col-md-6
      .h3 Last reviews
      %ul
        - current_user.reviews.last(5).each do |r|
          %li= r.content
